{
  "env": "Pendulum-v0",
  "model_type": "td3",
  "max_replay_memory_size": 100000,
  "use_gpu": false,
  "rl": {
    "gamma": 0.99,
    "target_update_rate": 0.005,
    "epsilon": 0
  },
  "td3_training": {
    "minibatch_size": 256,
    "use_2_q_functions": true,
    "noise_variance": 0.2,
    "noise_clip": 0.5,
    "delayed_policy_update": 2,
    "q_network_optimizer": {
      "learning_rate": 0.001
    },
    "actor_network_optimizer": {
      "learning_rate": 0.001
    }
  },
  "critic_training": {
    "layers": [128, 64],
    "activations": ["relu", "relu"]
  },
  "actor_training": {
    "layers": [128, 64],
    "activations": ["relu", "relu"]
  },
  "run_details": {
    "num_episodes": 1000,
    "max_steps": 1000,
    "train_every_ts": 1,
    "train_after_ts": 1,
    "test_every_ts": 2000,
    "test_after_ts": 1,
    "num_train_batches": 1,
    "avg_over_num_episodes": 100,
    "offline_train_epochs": 200
  }
}

{
  "env": "CartPole-v0",
  "model_type": "cross_entropy_method",
  "max_replay_memory_size": 20480,
  "use_gpu": false,
  "rl": {
    "gamma": 1.0,
    "softmax_policy": 0
  },
  "cem": {
    "mdnrnn": {
      "hidden_size": 100,
      "num_hidden_layers": 2,
      "minibatch_size": 1024,
      "learning_rate": 0.001,
      "not_terminal_loss_weight": 200.0,
      "next_state_loss_weight": 1.0,
      "reward_loss_weight": 1.0,
      "num_gaussians": 1
    },
    "plan_horizon_length": 10,
    "num_world_models": 1,
    "cem_population_size": 100,
    "cem_num_iterations": 10,
    "ensemble_population_size": 1,
    "num_elites": 15
  },
  "run_details": {
    "num_episodes": 200,
    "max_steps": 200,
    "train_every_ts": 1,
    "train_after_ts": 1,
    "test_every_ts": 500,
    "test_after_ts": 1,
    "num_train_batches": 1,
    "avg_over_num_episodes": 1,
    "offline_num_batches_per_epoch": 5000,
    "offline_train_epochs": 1
  }
}

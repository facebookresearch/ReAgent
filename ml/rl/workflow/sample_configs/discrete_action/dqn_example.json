{
  "training_data_path": "training_data/cartpole_discrete_timeline.json",
  "eval_data_path": "training_data/cartpole_discrete_timeline_eval.json",
  "state_norm_data_path": "training_data/state_features_norm.json",
  "model_output_path": "outputs/",
  "use_gpu": true,
  "use_all_avail_gpus": true,
  "norm_params": {
    "output_dir": "training_data/",
    "cols_to_norm": [
      "state_features"
    ],
    "num_samples": 1000
  },
  "actions": [
    "4",
    "5"
  ],
  "epochs": 100,
  "rl": {
    "gamma": 0.99,
    "target_update_rate": 0.2,
    "reward_burnin": 1,
    "maxq_learning": 1,
    "epsilon": 0.2,
    "temperature": 0.35,
    "softmax_policy": 0
  },
  "rainbow": {
    "double_q_learning": true,
    "dueling_architecture": false
  },
  "training": {
    "layers": [
      -1,
      128,
      64,
      -1
    ],
    "activations": [
      "relu",
      "relu",
      "linear"
    ],
    "minibatch_size": 256,
    "learning_rate": 0.001,
    "optimizer": "ADAM",
    "lr_decay": 0.999,
    "warm_start_model_path": null,
    "l2_decay": 0,
    "use_noisy_linear_layers": false
  }
}

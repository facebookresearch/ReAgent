{
  "env": "Asteroids-v0",
  "model_type": "discrete",
  "max_replay_memory_size": 100000,
  "rl": {
    "gamma": 0.99,
    "target_update_rate": 0.2,
    
    "maxq_learning": 1,
    "epsilon": 0.2,
    "temperature": 0.35,
    "softmax_policy": 0
  },
  "training": {
    "layers": [
      -1,
      128,
      64,
      -1
    ],
    "activations": [
      "relu",
      "relu",
      "linear"
    ],
    "minibatch_size": 64,
    "learning_rate": 0.001,
    "optimizer": "ADAM",
    "gamma": 0.999,
    "cnn_parameters": {
      "conv_dims": [
        3,
        32,
        16
      ],
      "conv_height_kernels": [
        8,
        4
      ],
      "conv_width_kernels": [
        8,
        4
      ],
      "pool_kernels_strides": [
        2,
        2
      ],
      "pool_types": [
        "max",
        "max"
      ]
    }
  },
  "run_details": {
    "num_episodes": 5001,
    "max_steps": 200,
    "train_every_ts": 1,
    "train_after_ts": 1,
    "test_every_ts": 2000,
    "test_after_ts": 1,
    "num_train_batches": 1,
    "avg_over_num_episodes": 100
  }
}

{
  "env": "Pendulum-v0",
  "model_type": "continuous",
  "max_replay_memory_size": 100000,
  "use_gpu": true,
  "rl": {
    "gamma": 0.99,
    "epsilon": 0,
    "target_update_rate": 0.1,
    "softmax_policy": 0
  },
  "shared_training": {
    "minibatch_size": 1024,
    "final_layer_init": 0.003,
    "optimizer": "ADAM"
  },
  "actor_training": {
    "layers": [
      -1,
      128,
      64,
      -1
    ],
    "activations": [
      "relu",
      "relu",
      "tanh"
    ],
    "learning_rate": 0.01
  },
  "critic_training": {
    "layers": [
      -1,
      128,
      64,
      -1
    ],
    "activations": [
      "relu",
      "relu",
      "linear"
    ],
    "learning_rate": 0.01,
    "l2_decay": 0.01
  },
  "run_details": {
    "num_episodes": 200,
    "max_steps": 1000,
    "train_every_ts": 1,
    "train_after_ts": 1,
    "test_every_ts": 2000,
    "test_after_ts": 1,
    "num_train_batches": 1,
    "avg_over_num_episodes": 100,
    "offline_train_epochs": 20
  }
}

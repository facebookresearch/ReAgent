{
  "env": "CartPole-v0",
  "model_type": "parametric",
  "rl": {
      "gamma": 0.99,
      "target_update_rate": 0.2,
      "reward_burnin": 1,
      "maxq_learning": 0,
      "epsilon": 0.0,
      "temperature": 0.35,
      "softmax_policy": 1
  },
  "training": {
      "layers": [
          -1,
          128,
          64,
          -1
      ],
      "activations": [
          "relu",
          "relu",
          "linear"
      ],
      "minibatch_size": 64,
      "learning_rate": 0.001,
      "optimizer": "ADAM",
      "gamma": 0.999
  },
  "run_details": {
      "num_episodes": 5001,
      "max_steps": 200,
      "train_every_ts": 1,
      "train_after_ts": 1,
      "test_every_ts": 2000,
      "test_after_ts": 1,
      "num_train_batches": 1,
      "avg_over_num_episodes": 100
    }
}
